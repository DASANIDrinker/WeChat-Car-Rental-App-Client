  /* 分类选择左侧导航栏 */
.leftOptions{
    /* !important代表使用外部样式类vant对于普通样式类的覆盖 */
    /* 一般来说 微信小程序无法确定外部样式和普通样式的优先级
    所以用!important来优先外部样式类 */
    color: #012ea5!important;
    line-height: 60rpx!important;
    font-size: 30rpx!important;
    font-weight: bold!important;
}
/* 分类选择右侧 */
.rightOptions{
    line-height: 80rpx!important;
}
/* 分类选择右侧被选中样式 */
.rightActiveOptions{
    line-height: 80rpx!important;
    color:#012ea5!important;
}
