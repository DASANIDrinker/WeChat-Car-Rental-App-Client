<!-- 东方时尚汽车租赁logo -->
<view class = "image">
    <image src="{{logoImg}}" mode="widthFix"></image>
</view>

<!-- 手机登录按钮 -->
<view>
    <van-button open-type="getPhoneNumber" bindgetphonenumber="getUserProfileByPhone" custom-class = "button">
    手机号一键登录
</van-button>
<van-toast id="van-toast" />
</view>


